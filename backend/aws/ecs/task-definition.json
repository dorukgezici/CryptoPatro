{
  "family": "crypto-adiutor",
  "executionRoleArn": "arn:aws:iam::919766826262:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "api",
      "image": "919766826262.dkr.ecr.eu-west-1.amazonaws.com/crypto-adiutor",
      "cpu": 256,
      "memoryReservation": 512,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::cryptopatro-secrets/backend-production.env",
          "type": "s3"
        }
      ],
      "environment": [
        {
          "name": "CRYPTOPATRO_CONTAINER_KIND",
          "value": "api"
        }
      ],
      "secrets": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/crypto-adiutor",
