{
  "family": "rabbitmq",
  "executionRoleArn": "arn:aws:iam::919766826262:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "rabbitmq",
      "image": "rabbitmq:3-management-alpine",
      "cpu": 256,
      "memoryReservation": 512,
      "portMappings": [
        {
          "containerPort": 5672,
          "hostPort": 5672,
          "protocol": "tcp"
        },
        {
          "containerPort": 15672,
          "hostPort": 15672,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "secrets": [
        {
          "name": "RABBITMQ_DEFAULT_VHOST",
          "valueFrom": "/crypto-adiutor/RABBITMQ_VHOST"
        },
        {
          "name": "RABBITMQ_DEFAULT_USER",
          "valueFrom": "/crypto-adiutor/RABBITMQ_USER"
        },
        {
          "name": "RABBITMQ_DEFAULT_PASS",
          "valueFrom": "/crypto-adiutor/RABBITMQ_PASSWORD"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/crypto-adiutor",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
